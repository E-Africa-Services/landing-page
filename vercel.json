{
  "rewrites": [
    {
      "source": "/amplify.app",
      "destination": "https://e-amplify.vercel.app/amplify.app"
    },
    {
      "source": "/amplify.app/:path*",
      "destination": "https://e-amplify.vercel.app/amplify.app/:path*"
    }
  ],
  "headers": [
    {
      "source": "/amplify.app/:path*",
      "headers": [
        { "key": "x-proxy-app", "value": "amplify" }
      ]
    }
  ]
}
